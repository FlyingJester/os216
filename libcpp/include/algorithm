#ifndef OS216_STDLIB_ALGORITHM
#define OS216_STDLIB_ALGORITHM
#pragma once

namespace std {

template<typename T>
const T& min(const T &a, const T &b){
    return (a > b) ? b : a;
}

template<typename T>
const T& max(const T &a, const T &b){
    return (a > b) ? a : b;
}



} // namespace std

#endif // OS216_STDLIB_ALGORITHM
